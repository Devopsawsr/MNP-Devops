pipeline {
    agent {label 'mnp-dev-app-agent'}
    stages {
        stage ('checkout') {
            steps {
                checkout scm
            }
        }
        stage ('Deploy') {
            steps {
                sh '''
                rm -rf /var/www/html/
                sudo cp /var/lib/jenkins/workspace/${JOB_NAME}/css.html /var/www/html/
                '''
            }
        }
    }
}
